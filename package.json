{
  "name": "veins-log-server",
  "version": "1.0.0",
  "description": "Veins V2X网络日志监控服务器",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "echo 'Server build complete'",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "setup": "npm install && npm run create-dirs",
    "create-dirs": "node scripts/create-directories.js",
    "check-deps": "npm audit && npm outdated",
    "clean": "rimraf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "veins",
    "v2x",
    "logs",
    "monitoring",
    "websocket",
    "express",
    "ca",
    "qca",
    "certificates"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-node": "^4.1.0",
    "pm2": "^5.3.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/veins-log-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/veins-log-monitor/issues"
  },
  "homepage": "https://github.com/your-username/veins-log-monitor#readme"
}