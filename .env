# 服务器配置
NODE_ENV=development
PORT=5000

# 日志配置
LOG_LEVEL=info
LOG_FILE_MAX_SIZE=100MB
LOG_FILE_MAX_FILES=10

# 日志文件路径
VEINS_LOG_PATH=d:\ACN\front\veins\examples\veins\logs
CA_LOG_PATH=d:\ACN\front\veins\cafiles\nodes
CERTIFICATES_PATH=d:\ACN\front\veins\cafiles
QCA_LOG_PATH=d:\ACN\front\veins\qca
CONFIG_PATH=d:\ACN\front\veins\examples\veins

# WebSocket配置
WEBSOCKET_PING_TIMEOUT=60000
WEBSOCKET_PING_INTERVAL=25000
MAX_CONNECTIONS=1000

# API配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
API_CACHE_DURATION=30000

# 安全配置
